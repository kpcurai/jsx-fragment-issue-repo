{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom", "dom.iterable"],
    "jsx": "preserve" ,
    "sourceMap": true ,
    "removeComments": true ,
    "downlevelIteration": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "./src" ,
    "esModuleInterop": true ,
    "resolveJsonModule": true ,
    "skipLibCheck": true,
    "types": [],
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
